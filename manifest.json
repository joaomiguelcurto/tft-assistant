{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "TFT Assistant",
    "author": "Miguel Janeiro & Joao Curto",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.160.0",
    "description": "Teamfight Tactics Assistant",
    "dock_button_title": "TFT Assistant",
    "icon": "icons/IconMouseNormal.png",
    "icon_gray": "icons/IconMouseOver.png",
    "launcher_icon": "icons/appIcon.ico",
    "window_icon": "icons/IconMouseNormal.png"
  },
  "permissions": [
    "Hotkeys",
    "GameInfo",
    "GameEvents"
  ],
  "data": {
    "start_window": "background",
    "hotkeys": {
      "sample_app_ts_showhide": {
        "title": "Show/Hide In-Game Window",
        "action-type": "toggle",
        "default": "Ctrl+F"
      }
    },
    "windows": {
      "background": {
        "file": "dist/background.html",
        "background_optimization": true,
        "is_background_page": true
      },
      "desktop": {
        "file": "dist/desktop.html",
        "desktop_only": true,
        "native_window": true,
        "resizable": true,
        "transparent": true,
        "override_on_update": true,
        "size": {
          "width": 1212,
          "height": 699
        },
        "min_size": {
          "width": 1212,
          "height": 699
        }
      },
      "in_game": {
        "file": "dist/in_game.html",
        "desktop_only": true,
        "native_window": true,
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "sample_app_ts_showhide",
        "resizable": true,
        "transparent": true,
        "override_on_update": true,
        "size": {
          "width": 1212,
          "height": 699
        },
        "min_size": {
          "width": 1212,
          "height": 699
        }
      },
      "debug": {
        "file": "dist/debug.html",
        "resizable": true,
        "transparent": true,
        "override_on_update": true,
        "desktop_only": false,
        "in_game_only": false,
        "size": {
          "width": 1212,
          "height": 699
        },
        "min_size": {
          "width": 1212,
          "height": 699
        }
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [5426]
    },
    "game_events": [5426],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [5426, 10902]
        },
        "start_minimized": true
      }
    ],
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}