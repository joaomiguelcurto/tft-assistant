{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "sourceMap": true,
    "outDir": "./dist",
    "moduleResolution": "Node",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "types": [
      "@overwolf/types"
    ],
    "rootDirs": [
      "./src"
    ],
  },
  "exclude": [
    "node_modules"
  ]
}
